{
  "name": "webpack-react",
  "version": "1.0.1",
  "author": "Graham Licence",
  "description": "React Webpack Base project",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "NODE_ENV=production webpack --config config/webpack/webpack.config.prod.js --progress --profile --colors",
    "start": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.dev.js --progress --profile --colors",
    "test": "BABEL_ENV=test mocha config/tests/mocha.config.js ./src/**/*.spec.js  --require babel-core/register --require babel-polyfill --exit",
    "lint": "npm run eslint && npm run sasslint",
    "preeslint": "echo 'Running eslint'",
    "eslint": "eslint --ext js --ext jsx src",
    "presasslint": "echo 'Running sasslint'",
    "sasslint": "sass-lint -c config/sass/.sass-lint.yml '**/*.scss' -v -q"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grahamlicence/base-projects"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "why-is-node-running": "2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "cssnano": "3.10.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.8",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.15.0-rc.3",
    "ignore-styles": "5.0.1",
    "jsdom": "11.6.2",
    "mocha": "5.0.0",
    "mockery": "2.1.0",
    "node-sass": "4.7.2",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-loader": "2.1.0",
    "prop-types": "15.6.0",
    "react-hot-loader": "3.1.3",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.6",
    "sass-resources-loader": "1.3.2",
    "sinon": "4.2.2",
    "style-loader": "0.20.2",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-common-shake": "1.5.3",
    "webpack-dev-server": "2.11.1"
  },
  "engines": {
    "node": "8.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  }
}
