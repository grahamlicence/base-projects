{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["last 2 versions", "safari >= 7"],
            },
            "modules": false,
        }],
        ["react"],
        ["stage-2"]
    ],
    "plugins": [
        "transform-runtime",
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        "transform-class-properties",
        "babel-plugin-transform-object-rest-spread"
    ]
}
